    <span id="breadCrumbing2Active"><%= @page_title %></span>
    <% else %>
    <span id="breadCrumbing2Active">Journals</span>
    <% end -%>
    </p>
    <!--close breadcrumbing2--></div>
  <div id="contentContainer3">

 <%= @content_for_layout %>
 
