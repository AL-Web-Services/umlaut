<% highlighted_links = get_service_type('highlighted_link')
   unless highlighted_links.empty? %>
<div class="seeAlso">
<div class="smGrayBox">
<strong>See also:</strong>
<% highlighted_link.each do |hl| 
    target = hl.service.to_highlighted_link
%>
<p class="tightbullets"><%= link_to target[:title], {:controller=>'link_router', :id=>hl.id}, :target=>'_blank' %>
</p>
<%end%>
</div>
</div>
<% end %>
