<% highlighted_links = get_service_type('highlighted_link')
   unless highlighted_links.blank? %>
<div class="seeAlso">
<div class="smGrayBox">
<strong>See also:</strong>
<% highlighted_links.each do |hl_st| 
    target = hl_st.view_data
%>
<p class="tightbullets"><%= link_to target[:display_text], {:controller=>'link_router', :id=>hl_st.id}, :target=>'_blank' %>
</p>
<%end%>
</div>
</div>
<% end %>
